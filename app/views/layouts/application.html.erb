<!DOCTYPE html>
<html>
  <head>
    <title>MusicProject</title>
    <%= csrf_meta_tags %>
    <%= csp_meta_tag %>

    <%= stylesheet_link_tag 'application', media: 'all', 'data-turbolinks-track': 'reload' %>
    <%= javascript_pack_tag 'application', 'data-turbolinks-track': 'reload' %>
  </head>

  <div>
    <% if logged_in? %>
    <%= link_to "Log Out", logout_path, method: :delete%><br>
    <%= link_to "Create New Artist", new_artist_path %><br>
    <%= link_to "All Artists", artists_path %><br>
    <%= link_to "All Genres", genres_path %><br>
    <%= link_to "All Saved EQ Settings", equalizers_path %><br>
    <% end %>
  </div>

  <body>
    <%= yield %>
  </body>
</html>
